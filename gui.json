{
  "title": "Painel Principal",
  "tabs": [
    {
      "id": "movement",
      "label": "Movimento",
      "components": [
        {
          "type": "toggle",
          "id": "speed_toggle",
          "label": "Speed",
          "moduleId": "speed",
          "onEnable": [
            { "action": "setPlayerSpeed", "value": "{speed.multiplier}" }
          ],
          "onDisable": [
            { "action": "setPlayerSpeed", "value": 1.0 }
          ]
        },
        {
          "type": "slider",
          "id": "speed_multiplier",
          "label": "Multiplicador",
          "moduleId": "speed",
          "property": "multiplier",
          "min": 1,
          "max": 10,
          "step": 1,
          "onChange": [
            { "action": "setPlayerSpeed", "value": "{speed.multiplier}" }
          ]
        },
        {
          "type": "toggle",
          "id": "fly_toggle",
          "label": "Fly",
          "moduleId": "fly",
          "onEnable": [
            { "action": "enableFly" }
          ],
          "onDisable": [
            { "action": "disableFly" }
          ]
        }
      ]
    },
    {
      "id": "render",
      "label": "Renderização",
      "components": [
        {
          "type": "toggle",
          "id": "fullbright_toggle",
          "label": "FullBright",
          "moduleId": "fullbright",
          "onEnable": [
            { "action": "setGamma", "value": 10.0 }
          ],
          "onDisable": [
            { "action": "setGamma", "value": 1.0 }
          ]
        }
      ]
    }
  ]
}
